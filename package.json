{
  "name": "generator-labs-koa-api",
  "version": "1.0.0",
  "description": "a Koa API generator",
  "homepage": "https://github.com/luanpablo-luizalabs/generator-labs-koa-api#readme",
  "author": "Luan Pablo",
  "files": [
    "generators"
  ],
  "main": "index.js",
  "keywords": [
    "koa",
    "api",
    "generator",
    "yeoman",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^2.13.1",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "scripts": {
    "test": "jest",
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luanpablo-luizalabs/generator-labs-koa-api.git"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/luanpablo-luizalabs/generator-labs-koa-api/issues"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  }
}
